{
  "version": "0.1.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn build-tailwind",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "myboostpow-lib",
  "author": "Guillaume De Martino",
  "module": "dist/myboostpow-lib.esm.js",
  "size-limit": [
    {
      "path": "dist/myboostpow-lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/myboostpow-lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "axios": "^1.2.2",
    "stag-relayx": "^0.2.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "husky": "^8.0.2",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.1.0",
    "tailwindcss": "^3.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
